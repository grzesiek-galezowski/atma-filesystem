<Window x:Class="PdfPresenter.HelperWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        Loaded="HelperWindow_OnLoaded"
        SizeChanged="HelperWindow_OnSizeChanged"
        mc:Ignorable="d"
        Title="HelperWindow" Height="300" Width="300">
        <Grid Name="HelperGrid">
            <Grid.RowDefinitions>
                <RowDefinition></RowDefinition>
                <RowDefinition></RowDefinition>
            </Grid.RowDefinitions>
            <Grid Name="HelpCurrentSlide" Grid.Row="0"></Grid>
            <Grid Name="HelpNextSlide" Grid.Row="1"></Grid>
        </Grid>
    
</Window>
